Sub exercicio05()

dim nota1 as double 
dim nota2 as double 
dim media as double 

nota1 = inputBox("Digite a primeira nota: ")
nota2 = inputbox("Digite a segunda nota: ")

media = (nota1 + nota2) / 2 

if media >= 7 and media <= 9.9 then 
	msgBox("Aprovado")
elseif media <= 7 then 
	msgBox("Reprovado")
elseif media = 10 then 
	msgBox("Aprovado com distinÃ§Ã£o")
endIf

End Sub
