Sub exercicio06()

dim n1 as integer 
dim n2 as integer 
dim n3 as integer 
dim maior as integer 

n1 = inputbox("Digite o numero: ")
n2 = inputbox("Digite o numero: ")
n3 = inputbox("Digite o numero: ")

maior = n1 


if n2 > maior then
	maior = n2 
elseif n3 > maior then 
	maior = n3
endif

msgbox("O maior numero eh: " &maior)
End Sub
